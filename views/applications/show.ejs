<%- include ('../partials/head.ejs') %>
  <body>
    <%- include ('../partials/nav.ejs') %>

    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%=application.companyName%></h5>
        <h6 class="card-subtitle mb-2 text-muted"><%=application.jobTitle%></h6>
        <p class="card-text">Location: <%=application.location%></p><br>
        <a href="www.google.com">Link To Job Posting</a>
        <p class="card-text">Link: <%=application.jobLink%></p><br>
        <p class="card-text">Rating: <%=application.desired%></p><br>
        <p class="card-text">Date Applied: <%=application.date%></p><br>
        <a class="card-link"href="/applications/<%=application.id%>/edit">Edit</a>
        <form action="/applications/<%=application._id%>?_method=DELETE" method="POST">
          <input type="submit" value="DELETE"/>
        </form>
      </div>

    </div>





  </body>
</html>
